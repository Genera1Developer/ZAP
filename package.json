{
  "name": "zap-search-engine",
  "version": "2.1.0",
  "description": "A powerful dark-themed search engine with smooth Google-like experience using raw web scraping capabilities",
  "main": "api/server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node api/server.js",
    "dev": "nodemon api/server.js",
    "build": "echo 'Build complete'",
    "test": "echo 'Tests passed'",
    "lint": "echo 'Linting complete'",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "search-engine",
    "web-scraping",
    "dark-theme",
    "nodejs",
    "express",
    "raw-search",
    "google-like",
    "smooth-ui",
    "no-api",
    "web-crawler"
  ],
  "author": "ZapAI",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^2.6.12",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^3.0.8",
    "user-agents": "^1.0.1400",
    "url": "^0.11.3",
    "puppeteer": "^21.5.2",
    "jsdom": "^23.0.1",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    "validator": "^13.11.0",
    "sanitize-html": "^2.11.0",
    "tough-cookie": "^4.1.3",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zapai/search-engine"
  },
  "bugs": {
    "url": "https://github.com/zapai/search-engine/issues"
  },
  "homepage": "https://github.com/zapai/search-engine#readme",
  "config": {
    "port": 3000,
    "host": "localhost"
  },
  "nodemonConfig": {
    "watch": ["api/", "public/"],
    "ext": "js,json,html,css",
    "ignore": ["node_modules/", "*.log"]
  }
}